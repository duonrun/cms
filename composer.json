{
	"name": "duon/cms",
	"description": "Duon content management system and framework",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Ernst",
			"email": "ernst@duon.run"
		}
	],
	"homepage": "https://duon.run/cms",
	"require": {
		"php": "^8.5",
		"ext-pgsql": "*",
		"ext-gd": "*",
		"ext-intl": "*",
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-xml": "*",
		"duon/core": "^0.1",
		"duon/error": "^0.1",
		"duon/quma": "^0.1",
		"duon/registry": "^0.1",
		"duon/session": "^0.1",
		"duon/sire": "^0.1",
		"gumlet/php-image-resize": "2.0.*",
		"hidehalo/nanoid-php": "^2",
		"laminas/laminas-httphandlerrunner": "^2.4",
		"psr/http-message-implementation": "^1.0",
		"symfony/html-sanitizer": "^7.1"
	},
	"require-dev": {
		"duon/cms-boiler": "^0.1",
		"duon/dev": "^2.5",
		"laminas/laminas-diactoros": "^3"
	},
	"autoload": {
		"psr-4": {
			"Duon\\Cms\\": "src"
		},
		"files": [
			"src/Util/Functions.php",
			"src/Field/Meta/Functions.php"
